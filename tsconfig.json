{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "strict": false,
        "noImplicitAny": false,
        "alwaysStrict": true,
        "strictBindCallApply": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": ".",
        "lib": [
            "esnext",
            "esnext.array",
            "dom",
            "dom.iterable",
            "es5"
        ]
    }
}